			},
			{
				"category": "Azure Spring App Settings",
				"command": "azureSpringApps.app.settings.edit",
				"title": "%azureSpringApps.common.edit%"
			},
			{
				"category": "Azure Spring Apps",
				"command": "azureSpringApps.common.toggleVisibility",
