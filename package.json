			},
			{
				"category": "Azure Spring App",
				"command": "azureSpringApps.app.delete",
				"icon": "$(remove)",
				"title": "%azureSpringApps.common.delete%"
			},
			{
				"category": "Azure Spring App",
				"command": "azureSpringApps.app.deploy",
				"icon": "$(cloud-upload)",
				"title": "%azureSpringApps.app.deploy%"
			},
			{
				"category": "Azure Spring App",
				"command": "azureSpringApps.app.scale",
				"icon": "$(settings)",
